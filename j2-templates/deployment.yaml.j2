apiVersion: apps/v1
kind: Deployment
metadata:
  labels:
    {% raw %}{{- include "helm.labels" . | nindent 4 }}{% endraw %}
  name: {% raw %}{{ .Chart.Name }}{% endraw %}
spec:
  replicas: 1
  selector:
    matchLabels:
      app: mytest
  template:
    metadata:
      labels:
        app: mytest
    spec:
      containers:
      - image: robertsilvatech/flask-color
        name: flask-color
